<?xml version="1.0"?>
<config>
    <modules>
        <Simi_Simimigrate>
            <version>0.1.0</version>
        </Simi_Simimigrate>
    </modules>
    <frontend>
        <routers>
            <simimigrate>
                <use>standard</use>
                <args>
                    <module>Simi_Simimigrate</module>
                    <frontName>simimigrate</frontName>
                </args>
            </simimigrate>
        </routers>
        <layout>
            <updates>
                <simimigrate>
                    <file>simimigrate.xml</file>
                </simimigrate>
            </updates>
        </layout>
    </frontend>
    <global>
        <helpers>
            <simimigrate>
                <class>Simi_Simimigrate_Helper</class>
            </simimigrate>
        </helpers>
        <models>
            <simimigrate>
                <class>Simi_Simimigrate_Model</class>
                <resourceModel>simimigrate_mysql4</resourceModel>
            </simimigrate>
            <simimigrate_mysql4>
                <class>Simi_Simimigrate_Model_Mysql4</class>
                <entities>
                    <app>
                        <table>simimigrate_app</table>
                    </app>
                    <store>
                        <table>simimigrate_store</table>
                    </store>
                    <storeview>
                        <table>simimigrate_storeview</table>
                    </storeview>
                    <category>
                        <table>simimigrate_category</table>
                    </category>
                    <product>
                        <table>simimigrate_product</table>
                    </product>
                    <customer>
                        <table>simimigrate_customer</table>
                    </customer>
                </entities>
            </simimigrate_mysql4>
        </models>
        <resources>
            <simimigrate_setup>
                <setup>
                    <module>Simi_Simimigrate</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </simimigrate_setup>
            <simimigrate_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </simimigrate_write>
            <simimigrate_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </simimigrate_read>
        </resources>
    </global>
</config> 